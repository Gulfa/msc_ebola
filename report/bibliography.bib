	keywords = {Infectious diseases, Disease dynamics, Human mobility, Influenza, Norway, Rural areas, Urban areas, Vaccination and immunization},
	pages = {e1006879},
	file = {Full Text PDF:/home/gunnar/Zotero/storage/UA24PIGC/Engebretsen et al. - 2019 - A theoretical single-parameter model for urbanisat.pdf:application/pdf;Snapshot:/home/gunnar/Zotero/storage/JHQ5VWAE/article.html:text/html}
