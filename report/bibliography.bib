	file = {ScienceDirect Full Text PDF:/home/gunnar/Zotero/storage/FAXWTHGG/Zhao et al. - 2020 - Dynamics of a time-periodic two-strain SIS epidemi.pdf:application/pdf;ScienceDirect Snapshot:/home/gunnar/Zotero/storage/NQDQLGLN/S1468121819301713.html:text/html}
}

@article{garnettPL06MathematicalModelling2019,
	title = {{PL}06.1 {Mathematical} modelling, a luxury or a necessity in {STD} control?},
