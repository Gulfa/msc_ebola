	pages = {e0184419},
	file = {Full Text PDF:/home/gunnar/Zotero/storage/MJ5NDEIN/Mathes et al. - 2017 - Evaluating and implementing temporal, spatial, and.pdf:application/pdf;Snapshot:/home/gunnar/Zotero/storage/EPNFWLAG/article.html:text/html}
