	note = {arXiv: 1907.07464},
	keywords = {Quantitative Biology - Quantitative Methods, Computer Science - Machine Learning, Statistics - Machine Learning},
	file = {arXiv\:1907.07464 PDF:/home/gunnar/Zotero/storage/GGUXBXTJ/Kulessa et al. - 2019 - Improving Outbreak Detection with Stacking of Stat.pdf:application/pdf;arXiv.org Snapshot:/home/gunnar/Zotero/storage/SL6X7NTT/1907.html:text/html}
}

@article{edo-osagieTwitterMiningUsing2019,
