@article{barryOutbreakEbolaVirus2018,
	title = {Outbreak of {Ebola} virus disease in the {Democratic} {Republic} of the {Congo}, {April}–{May}, 2018: an epidemiological study},
	volume = {392},
	issn = {0140-6736, 1474-547X},
	shorttitle = {Outbreak of {Ebola} virus disease in the {Democratic} {Republic} of the {Congo}, {April}–{May}, 2018},
	url = {https://www.thelancet.com/journals/lancet/article/PIIS0140-6736(18)31387-4/abstract},
	doi = {10.1016/S0140-6736(18)31387-4},
	abstract = {{\textless}h2{\textgreater}Summary{\textless}/h2{\textgreater}{\textless}h3{\textgreater}Background{\textless}/h3{\textgreater}{\textless}p{\textgreater}On May 8, 2018, the Government of the Democratic Republic of the Congo reported an outbreak of Ebola virus disease in Équateur Province in the northwest of the country. The remoteness of most affected communities and the involvement of an urban centre connected to the capital city and neighbouring countries makes this outbreak the most complex and high risk ever experienced by the Democratic Republic of the Congo. We provide early epidemiological information arising from the ongoing investigation of this outbreak.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Methods{\textless}/h3{\textgreater}{\textless}p{\textgreater}We classified cases as suspected, probable, or confirmed using national case definitions of the Democratic Republic of the Congo Ministère de la Santé Publique. We investigated all cases to obtain demographic characteristics, determine possible exposures, describe signs and symptoms, and identify contacts to be followed up for 21 days. We also estimated the reproduction number and projected number of cases for the 4-week period from May 25, to June 21, 2018.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Findings{\textless}/h3{\textgreater}{\textless}p{\textgreater}As of May 30, 2018, 50 cases (37 confirmed, 13 probable) of \textit{Zaire ebolavirus} were reported in the Democratic Republic of the Congo. 21 (42\%) were reported in Bikoro, 25 (50\%) in Iboko, and four (8\%) in Wangata health zones. Wangata is part of Mbandaka, the urban capital of Équateur Province, which is connected to major national and international transport routes. By May 30, 2018, 25 deaths from Ebola virus disease had been reported, with a case fatality ratio of 56\% (95\% CI 39–72) after adjustment for censoring. This case fatality ratio is consistent with estimates for the 2014–16 west African Ebola virus disease epidemic (p=0·427). The median age of people with confirmed or probable infection was 40 years (range 8–80) and 30 (60\%) were male. The most commonly reported signs and symptoms in people with confirmed or probable Ebola virus disease were fever (40 [95\%] of 42 cases), intense general fatigue (37 [90\%] of 41 cases), and loss of appetite (37 [90\%] of 41 cases). Gastrointestinal symptoms were frequently reported, and 14 (33\%) of 43 people reported haemorrhagic signs. Time from illness onset and hospitalisation to sample testing decreased over time. By May 30, 2018, 1458 contacts had been identified, of which 746 (51\%) remained under active follow-up. The estimated reproduction number was 1·03 (95\% credible interval 0·83–1·37) and the cumulative case incidence for the outbreak by June 21, 2018, is projected to be 78 confirmed cases (37–281), assuming heterogeneous transmissibility.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Interpretation{\textless}/h3{\textgreater}{\textless}p{\textgreater}The ongoing Ebola virus outbreak in the Democratic Republic of the Congo has similar epidemiological features to previous Ebola virus disease outbreaks. Early detection, rapid patient isolation, contact tracing, and the ongoing vaccination programme should sufficiently control the outbreak. The forecast of the number of cases does not exceed the current capacity to respond if the epidemiological situation does not change. The information presented, although preliminary, has been essential in guiding the ongoing investigation and response to this outbreak.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Funding{\textless}/h3{\textgreater}{\textless}p{\textgreater}None.{\textless}/p{\textgreater}},
	language = {English},
	number = {10143},
	urldate = {2019-09-08},
	journal = {The Lancet},
	author = {Barry, Ahmadou and Ahuka-Mundeke, Steve and Ahmed, Yahaya Ali and Allarangar, Yokouide and Anoko, Julienne and Archer, Brett Nicholas and Abedi, Aaron Aruna and Bagaria, Jayshree and Belizaire, Marie Roseline Darnycka and Bhatia, Sangeeta and Bokenge, Théophile and Bruni, Emanuele and Cori, Anne and Dabire, Ernest and Diallo, Amadou Mouctar and Diallo, Boubacar and Donnelly, Christl Ann and Dorigatti, Ilaria and Dorji, Tshewang Choden and Waeber, Aura Rocio Escobar Corado and Fall, Ibrahima Socé and Ferguson, Neil M. and FitzJohn, Richard Gareth and Tengomo, Gervais Leon Folefack and Formenty, Pierre Bernard Henri and Forna, Alpha and Fortin, Anne and Garske, Tini and Gaythorpe, Katy AM and Gurry, Celine and Hamblion, Esther and Djingarey, Mamoudou Harouna and Haskew, Christopher and Hugonnet, Stéphane Alexandre Louis and Imai, Natsuko and Impouma, Benido and Kabongo, Guylain and Kalenga, Oly Ilunga and Kibangou, Emerencienne and Lee, Theresa Min-Hyung and Lukoya, Charles Okot and Ly, Ousmane and Makiala-Mandanda, Sheila and Mamba, Augustin and Mbala-Kingebeni, Placide and Mboussou, Franck Fortune Roland and Mlanda, Tamayi and Makuma, Vital Mondonge and Morgan, Oliver and Mulumba, Anastasie Mujinga and Kakoni, Patrick Mukadi and Mukadi-Bamuleka, Daniel and Muyembe, Jean-Jaques and Bathé, Ndjoloko Tambwe and Ngamala, Patricia Ndumbi and Ngom, Roland and Ngoy, Guillaume and Nouvellet, Pierre and Nsio, Justus and Ousman, Kevin Babila and Peron, Emilie and Polonsky, Jonathan Aaron and Ryan, Michael J. and Touré, Alhassane and Towner, Rodney and Tshapenda, Gaston and Weerdt, Reinhilde Van De and Kerkhove, Maria Van and Wendland, Annika and Yao, N'Da Konan Michel and Yoti, Zabulon and Yuma, Etienne and Kabamba, Guy Kalambayi and Mwati, Jean de Dieu Lukwesa and Mbuy, Gisele and Lubula, Leopold and Mutombo, Anny and Mavila, Oscar and Lay, Yyonne and Kitenge, Emma},
	month = jul,
